<div class="col-md-9">
  <div class="panel panel-primary">
    <div class="panel-heading">
      Artists
    </div>
    <div class="panel-body">

      <% @artist.each do |artist| %>
      <div class="row">
        <div class="col-md-7">
          <h4><%= artist.name %></h4>

          <% end %>
          <h4>Photos</h4>
        <% @artist.photos.each do |photo| %>
          <%= image_tag photo.image %>
        <% end %>

        </div>
        <div class="col-md-3 right">

        </div>
      </div>


    </div>
  </div>
