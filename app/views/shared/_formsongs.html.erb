
<%= form_for @songs do |f| %>

  <div class="form-group">
    <label> Title of the Song: </label>
     <%= f.text_field :titel, placeholder: "Song title", data: {artist: @artist.id}, class: "form-control" %>
    <%= f.submit "- Save -", id: "submit-button", class: "btn btn-success", data: { disable_with: false }%>
  </div>
<% end %>
